CREATE TABLE USER (
ID INT PRIMARY KEY,
TITLE VARCHAR2(250),
FIRST_NAME VARCHAR2(250),
SECOND_NAME VARCHAR2(250),
FIRST_LAST_NAME VARCHAR2(250),
SECOND_LAST_NAME VARCHAR2(250),
DOCUMENT_TYPE VARCHAR2(250),
EXPEDITION_PLACE VARCHAR2(250),
EXPEDITION_DATE VARCHAR2(250),
DOCUMENT_NUMBER VARCHAR2(250),
NACIONALITY VARCHAR2(250),
BORN_DATE DATE,
BORN_PLACE VARCHAR2(250),
PASSWORD VARCHAR2(250),
BALANCE DECIMAL
STATE VARCHAR2(250) );

CREATE TABLE EMAIL (
ID INT PRIMARY KEY,
EMAIL VARCHAR2(256),
PASSWORD VARCHAR2(256),
LANGUAGE VARCHAR2(256) 
STATE VARCHAR2(256) );

CREATE TABLE PHONE (
ID INT PRIMARY KEY,
CODE INT,
NUMBER VARCHAR2(12)
STATE VARCHAR2(256) );

CREATE TABLE COUNTRY (
ID INT PRIMARY KEY,
NAME VARCHAR2(256),
STATE VARCHAR2(256) );

CREATE TABLE DEPARTMENT (
ID INT PRIMARY KEY,
NAME VARCHAR2(256),
STATE VARCHAR2(256)  );

CREATE TABLE CITY (
ID INT PRIMARY KEY,
NAME VARCHAR2(256),
STATE VARCHAR2(256)  );

CREATE TABLE BONUS (
ID INT PRIMARY KEY,
CODE INT,
VALUES DECIMAL,
STATE VARCHAR2(250) );

CREATE TABLE ACCOUNT(
ID INT PRIMARY KEY,
BANK VARCHAR2(255),
ACCOUNT_NUMBER VARCHAR2(255),
STATE VARCHAR2(256) );


CREATE TABLE AUDIT(
ID INT PRIMARY KEY,
DATE_TIME DATATIME,
TABLE VARCHAR2(255),
ACTION VARCHAR2(255),
USER_ID INT FOREING KEY,
USER_NAME VARCHAR2(255),
STATE VARCHAR2(255) );

CREATE TABLE WITHDRAW (
ID INT PRIMARY KEY,
REQUEST VARCHAR2(255),
DATE_REQUEST DATE,
DATE_ DISBURSEMENT DATE,
CAPACITY VARCHAR2(255),
STATE VARCHAR2(250) );

CREATE TABLE CONTACT_DATA(
ID INT PRIMARY KEY,
RESIDENCE_ADDRESS_1 VARCHAR2(255),
RESIDENCE_ADDRESS_2 VARCHAR2(255),
POSTAL_CODE INT,
RESIDENCE_DEPARTMENT VARCHAR2(255),
RESIDENCE_CITY VARCHAR2(255),
RESIDENCE_COUNTRY VARCHAR2(255),
STATE VARCHAR2(250) );


CREATE TABLE VOUCHER(
ID INT PRIMARY KEY,
ARCHIVE_SIZE VARCHAR2(255),
ARCHIVE_URL VARCHAR2(255),
ARCHIVE_EXTENSION VARCHAR2(255),
STATE VARCHAR2(250) );

CREATE TABLE DEPOSIT(
ID INT PRIMARY KEY,
VALUE DECIMAL,
METHOD VARCHAR2(250),
DATA VARCHAR2(250),
ACCION VARCHAR2(250),
DATE DATE,
DEPOSIT_STATE VARCHAR2(250),
STATE2 VARCHAR2(250) );

CREATE TABLE DEPOSIT_LIMIT(
ID INT PRIMARY KEY,
UNITY VARCHAR2(250),
AMOUNT DECIMAL,
TYPE VARCHAR2(250),
AVAILABLE_TOTAL DECIMAL,
STATE_LIMIT VARCHAR2(250),
DATE DATE,
TIME TIME,
SESSION_TIME TIME,
CODITION VARCHAR2(250),
STATE VARCHAR2(250) );

CREATE TABLE BET(
ID INT PRIMARY KEY,
BET_STATE VARCHAR2(250),
BET VARCHAR2(250),
BET_VALUE DECIMAL,
MAXIME_PAYMENT DECIMAL,
BET_TYPE VARCHAR2(250),
STATE VARCHAR2(250) );

CREATE TABLE PREFERENCE(
ID INT PRIMARY KEY,
NEWSLETTER VARCHAR2(255),
BET_EMAIL VARCHAR2(255),
SMS-SEND VARCHAR2(255),
BROWSER_NOTIFICATION VARCHAR2(255),
NOTICE_BULLETIN VARCHAR2(255),
STATE_REFERENCE VARCHAR2(255),
STATE VARCHAR2(250) );

CREATE TABLE FOOTBALL_GAMES (
ID INT PRIMARY KEY,
TEAM_1 VARCHAR2(255),
TEAM_2 VARCHAR2(255),
RESULTADO VARCHAR2(255),
STATE VARCHAR2(250) );



CREATE TABLE USER_BET(
ID INT PRIMARY KEY,
USER_ID INT,
BET_ID INT,
STATE VARCHAR2(250) );
